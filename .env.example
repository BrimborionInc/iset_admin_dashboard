# Copy to .env (which is gitignored) and fill values
# Server basics
NODE_ENV=development
PORT=5001
ALLOWED_ORIGIN=http://localhost:3000,http://localhost:3001
AUTH_PROVIDER=none  # or cognito

# Database
DB_HOST=localhost
DB_PORT=3306
DB_USER=root
DB_PASS=changeme
DB_NAME=iset_admin

# Optional dev tooling
ENABLE_DEV_DB_INSPECTOR=false
DEV_DB_KEY=
ENABLE_DEV_SEED_TEMPLATES=false

# OpenRouter / AI
OPENROUTER_API_KEY=
OPENROUTER_KEY=

# Cognito / AWS Auth (backend)
AWS_REGION=ca-central-1
COGNITO_REGION=ca-central-1
COGNITO_USER_POOL_ID=
COGNITO_CLIENT_ID=
COGNITO_JWKS_URL=
COGNITO_ISSUER=
COGNITO_DOMAIN=
COGNITO_REDIRECT_URI=http://localhost:3001/auth/callback

# Cognito (frontend React expects REACT_APP_*)
REACT_APP_API_BASE_URL=http://localhost:5001
REACT_APP_AWS_REGION=ca-central-1
REACT_APP_COGNITO_DOMAIN_PREFIX=
REACT_APP_COGNITO_CLIENT_ID=
REACT_APP_COGNITO_REDIRECT_URI=http://localhost:3001/auth/callback
REACT_APP_COGNITO_LOGOUT_URI=http://localhost:3001/

# Dev auth bypass (non-production only)
DEV_AUTH_BYPASS=false
DEV_AUTH_TOKEN=
DEV_AUTH_OPEN=false
DEV_AUTH_RELAXED=false

# Misc / Scripts
EXTERNAL_SITE=
API_BASE=http://localhost:5001
CODE=
